{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ChatGpt": {
    "ApiKey": "",
    "DefaultModel": "gpt-3.5-turbo",
    "MaxTokens": 1000,
    "Temperature": 0.7,
    "TimeoutSeconds": 30
  },
  "DocumentProcessing": {
    "MaxFileSizeMB": 50,
    "DefaultChunkSize": 1000,
    "DefaultChunkOverlap": 200,
    "DefaultExpirationHours": 2,
    "MaxExpirationHours": 24,
    "ProcessingTimeoutMinutes": 30
  },
  "Console": {
    "ShowProgressIndicator": true,
    "MaxQueryHistory": 50,
    "DefaultPageWidth": 80
  }
}
